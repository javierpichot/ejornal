{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "acacha/admin-lte-template-laravel": "2.*",
        "bacon/bacon-qr-code": "^1.0.3",
        "barryvdh/laravel-dompdf": "^0.8.3",
        "caffeinated/shinobi": "^3.2",
        "fideloper/proxy": "^4.0",
        "intervention/image": "^2.4",
        "jenssegers/date": "^3.4",
        "lab404/laravel-impersonate": "^1.2",
        "laravel/framework": "5.6.*",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "^5.4.0",
        "laravelista/ekko": "^1.5",
        "maatwebsite/excel": "~3.1",
        "maddhatter/laravel-fullcalendar": "^1.3",
        "nesbot/carbon": "1.36.2",
        "owen-it/laravel-auditing": "^8.0",
        "paragonie/constant_time_encoding": "^2.2",
        "pragmarx/google2fa-laravel": "^0.2.0",
        "spatie/laravel-url-signer": "^2.2",
        "spatie/pdf-to-image": "^1.8",
        "webklex/laravel-imap": "^1.2",
        "yajra/laravel-datatables-oracle": "~8.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.1",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-install-cmd": [
            "ln -s storage/empresas public/storage/empresas",
            "ln -s storage/jornal public/storage/jornal",
            "ln -s storage/profesionales public/storage/profesionales",
            "ln -s storage/trabajadores public/storage/trabajadores"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
